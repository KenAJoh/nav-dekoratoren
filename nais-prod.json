{
    "apiVersion": "nais.io/v1alpha1",
    "kind": "Application",
    "metadata": {
        "name": "nav-dekoratoren",
        "namespace": "default",
        "labels": {
            "team": "personbruker"
        }
    },
    "spec": {
        "image": "navikt/nav-dekoratoren:latest",
        "port": 8088,
        "liveness": {
            "path": "/person/nav-dekoratoren/isalive",
            "initialDelay": 10
        },
        "readiness": {
            "path": "/person/nav-dekoratoren/isready",
            "initialDelay": 10
        },
        "prometheus": {
            "path": "/metrics"
        },
        "replicas": {
            "min": 2,
            "max": 4
        },
        "ingresses": [
            "https://nav-dekoratoren.nais.oera.no",
            "https://www.nav.no/person/nav-dekoratoren"
        ],
        "env": [
            {
                "name": "MENYLENKER",
                "value": "https://www-x1.nav.no"
            }
        ],
        "resources": {
            "limits": {
                "cpu": "3",
                "memory": "768Mi"
            },
            "requests": {
                "cpu": "200m",
                "memory": "512Mi"
            }
        }
    }
}
