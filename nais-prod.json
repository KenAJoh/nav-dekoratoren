{
  "apiVersion": "nais.io/v1alpha1",
  "kind": "Application",
  "metadata": {
    "name": "nav-dekoratoren",
    "namespace": "default",
    "labels": {
      "team": "personbruker"
    }
  },
  "spec": {
    "image": "navikt/nav-dekoratoren:latest",
    "port": 8080,
    "liveness": {
      "path": "/isalive",
      "initialDelay": 10
    },
    "readiness": {
      "path": "/isready",
      "initialDelay": 10
    },
    "prometheus": {
      "path": "/metrics"
    },
    "replicas": {
      "min": 2,
      "max": 4
    },
    "ingresses": [
      "https://nav-dekoratoren.nais.oera.no"
    ],
    "env": [
      {
        "name": "MY_CUSTOM_VAR",
        "value": "some_value"
      }
    ],
    "resources": {
      "limits": {
        "cpu": "3",
        "memory": "768Mi"
      },
      "requests": {
        "cpu": "200m",
        "memory": "512Mi"
      }
    }
  }
}